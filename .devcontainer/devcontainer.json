{
	"name": "cloudwatch-read-adapter",
	"dockerComposeFile": [
		"./docker-compose.yml"
	],
	"service": "cloudwatch-read-adapter",
	"runServices": [
		"prometheus"
	],
	"workspaceFolder": "/workspace",
	"remoteUser": "vscode",
	"postAttachCommand": ".devcontainer/postAttach.sh",
	"containerEnv": {
		"SHELL": "/bin/bash"
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"golang.Go"
			]
		}
	}
}
